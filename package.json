{
  "name": "ts-node-react-starter",
  "version": "1.0.0",
  "engines": {
    "node": "12.11.1"
  },
  "description": "Starter project for Typescript NodeJS.",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "dev:client": "npm run dev --prefix=src-client",
    "dev-all": "concurrently \"npm run dev\" \"npm run dev --prefix src-client\"",
    "build-all": "npm run build && npm run build --prefix src-client",
    "lint-all": "tslint -c tslint.json -p tsconfig.json && npm run lint --prefix src-client"
  },
  "author": "deskoh",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/mongodb": "^3.3.11",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.7.13",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "source-map-loader": "^0.2.4",
    "ts-node": "^7.0.1",
    "tslib": "^1.10.0",
    "tslint": "^5.12.1",
    "typescript": "^3.1.6",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/mongodb": "^3.3.11",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.14",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0"
  }
}
